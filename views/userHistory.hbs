<div class="container">
  </br>
  <div class="row">
    <a href="/logout" class="btn btn-danger">Logout</a>
    <div class="col-md-12">
      </br>
      <h2> {{session.user.username}} ( {{session.user.email}} )  History</h2>
      </hr>
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Login Date/Time</th>
            <th scope="col">Client Information</th>
          </tr>
        </thead>
        <tbody>
          {{#each session.user.loginHistory}}
          <tr>
            <td>{{dateTime}}</td>
            <td>{{userAgent}}</td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>